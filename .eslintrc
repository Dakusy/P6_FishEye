{
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
        "no-unused-vars": "off",
        "no-undef": "off",
        "no-useless-escape" : "off"
      },
    "extends": [
      "eslint:recommended"
    ]
  }